<header class="header" about="{{about}}" typeof="{{typeof}}" role="banner">
	{{#logo.parameters.showLogoOnHeader}}
  	<div class="logo">
    	<h1>
    		<a href="{{logo.link.path}}" data-tag="wh-homepage-click">
				{{{logo.image.imageHtml}}}				
    		</a>	
    	</h1>
  	</div>
  	{{/logo.parameters.showLogoOnHeader}}
	<ul class="utility-nav unstyled">
		<li class="sign-in-btn"><a href="{{user.link.path}}" class="sign-in button small" dataTag="wh-sign-out-click">{{{user.link.title}}}</a></li>
		<li class="language-selection">
		{{#languages}}
		  {{#parameters.showLanguages}}
		  {{#parameters.showByIcon}}
			<a href="javascript: void(0);" class="button language dropdown menu-link flag-link small" data-dropdown="language-dropdown">{{{languages.current.code}}}<img class="flag-img" src="{{languages.current.src}}" alt="{{{languages.current.alt}}}" width="30" height="18"></a>
			<ul id="language-dropdown" class="f-dropdown" data-dropdown-content="">
			  {{#languages}}				  
			  <li class="language-option{{#selected}} active{{/selected}}">
				<a class="flag-link" href="javascript: void(0);" title="{{description}}" onclick="changeLanguage(this, {{parameters.promptBeforeChangeLanguage}}, '{{labels.general.confirmMessagechangelanguage}}', {{value}});" data-tag="wh-change-language-click">
					{{{code}}}
					<img class="flag-img" src="{{src}}" alt="{{{alt}}}" width="30" height="18">
				</a>
			  </li>
			  {{/languages}}
			</ul>
		  {{/parameters.showByIcon}}
		  {{^parameters.showByIcon}}
			<a href="javascript: void(0);" class="button language dropdown menu-link small" data-dropdown="language-dropdown">{{{languages.current.description}}}</a>
			<ul id="language-dropdown" class="f-dropdown" data-dropdown-content="">
			  {{#languages}}
			  <li class="language-option{{#selected}} active{{/selected}}">
				<a href="javascript: void(0);" onclick="changeLanguage(this, {{parameters.promptBeforeChangeLanguage}}, '{{labels.general.confirmMessagechangelanguage}}', {{value}});" data-tag="wh-change-language-click">
				{{{description}}}
				</a>
			  </li>
			  {{/languages}}
			</ul>
		  {{/parameters.showByIcon}}
		  {{/parameters.showLanguages}}
		{{/languages}}
		</li>
	</ul>
	<div class="primary-nav">
	  <nav class="top-bar" data-topbar>
		<ul class="title-area">
		  <li class="name">
		  </li>
		  <li class="toggle-topbar menu-icon"><a href="#">{{{labels.general.menu}}}</a></li>
		</ul>
		<section class="top-bar-section">
		  <ul class="module unstyled">
		  	{{#header.navigation}}
			<li id="{{key}}"><a id="{{value.id}}" href="{{value.path}}" data-tag="{{value.dataTag}}" {{#value.target}}target="{{.}}"{{/value.target}}>{{{value.title}}}</a></li>
			{{/header.navigation}}		  
		  </ul>
		</section>
	  </nav>
	</div>
</header>